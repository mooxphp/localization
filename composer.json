{
    "name": "moox/localization",
    "description": "This is my package localization",
    "keywords": [
        "Laravel",
        "Filament",
        "Filament plugin",
        "Laravel package"
    ],
    "homepage": "https://moox.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Moox Developer",
            "email": "dev@moox.org",
            "role": "Developer"
        }
    ],
    "require": {
        "moox/core": "5.0.12",
        "moox/data": "5.0.12",
        "astrotomic/laravel-translatable": "^11.16"
    },
    "autoload": {
        "psr-4": {
            "Moox\\Localization\\": "src",
            "Moox\\Localization\\Database\\Factories\\": "database/factories",
            "Moox\\Localization\\Database\\Seeders\\": "database/seeders"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moox\\Localization\\LocalizationServiceProvider"
            ]
        },
        "moox": {
            "stability": "stable"
        }
    },
    "prefer-stable": true,
    "require-dev": {
        "moox/devtools": "5.0.12"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}